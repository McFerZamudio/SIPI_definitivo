            </td>
            <td>
                @Html.DisplayFor(modelItem => item.id_equipoNavigation.equipo_nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.id_estudianteEstatusNavigation.estudianteEstatus_nombre)
            </td>
            @*<td>
                <a asp-action="Edit" asp-route-id="@item.id_estudiante">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_estudiante">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_estudiante">Delete</a>
            </td>*@
