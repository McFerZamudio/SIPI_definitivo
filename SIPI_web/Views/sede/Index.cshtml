@model IEnumerable<SIPI_web.Models.tbl_sede>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.sede_codigo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sede_nombre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.sede_codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sede_nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_sede">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_sede">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_sede">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<a asp-action="Index" asp-controller="tablasPrevias">Regresar a Tablas Previas</a>
