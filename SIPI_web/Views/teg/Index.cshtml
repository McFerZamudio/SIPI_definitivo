                <td>
                    @Html.DisplayFor(modelItem => item.teg_observacionRecepcionDocumento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.id_consultorAcademicoNavigation.id_persona)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.id_consultorMetodologiaNavigation.id_persona)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.id_tegNavigation.trabajo_planteamientoProblema)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.id_teg">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.id_teg">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.id_teg">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
