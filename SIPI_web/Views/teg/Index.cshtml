            <th>
                @Html.DisplayNameFor(model => model.teg_codigoInterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.teg_codigoTeg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.teg_porcentaje)
