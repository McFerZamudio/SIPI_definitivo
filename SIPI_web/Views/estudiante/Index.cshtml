@model IEnumerable<SIPI_web.Models.tbl_estudiante>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutPublico.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombre Completo
            </th>
            <th>
               Fechas Importantes
            </th>
            <th>
               Cohorte
            </th>
            <th>
               Equipo
            </th>
              @*<th></th>*@
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_estudianteNavigation.peresona_nombreCompleto) <br />
