@model SIPI_web.Models.tbl_integrante

@{
    ViewData["Title"] = "Create";
    Layout = "";
}

<div class="row">
    <div>
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" id="id_trabajo" name="id_trabajo" asp-for="id_trabajo" />
            <input type="hidden" id="id_estudiante" name="id_estudiante" asp-for="id_estudiante"  value="ViewBag.id_estudiante" />
            <div class="form-group">
                <label asp-for="integrantes_fechaCarga" class="control-label"></label>
                <input asp-for="integrantes_fechaCarga" class="form-control" readonly/>
                <span asp-validation-for="integrantes_fechaCarga" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="integrandes_fechaConfirmado" class="control-label"></label>
                <input asp-for="integrandes_fechaConfirmado" class="form-control" readonly/>
                <span asp-validation-for="integrandes_fechaConfirmado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
